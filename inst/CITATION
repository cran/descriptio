citHeader("To cite GDAtools in publications, use:")

bibentry(
  bibtype  = "manual",
  title    = "{descriptio}: Descriptive Analysis of Associations between Variables in {R}",
  author   = "Nicolas Robette",
  year     = "2025",
  note     = "R package version 1.4",
  url      = "https://nicolas-robette.github.io/descriptio/",
  
  textVersion = paste("Robette N. (2025).",
        "descriptio: Descriptive Analysis of Associations between Variables in R.",
        "version 1.5.",
        "https://nicolas-robette.github.io/descriptio/")
)

citFooter("See",sQuote("citation()"),"for citing R itself.")

